{
    "compilerOptions": {
        "outDir": "./wwwroot/dist/",
        "noImplicitAny": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "lib": ["es6", "dom"],
        "target": "es5",
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
